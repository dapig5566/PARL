{
    "disable_env_checking": true,
    "env": "OrderEnv-v2",
    "env_config": {
        "num_cars": 3,
        "num_couriers": 1,
        "max_episode_length": 300,
        "wrap_obs": false, 
        "use_gymnasium": true,
        "ablation": null,
        "random_episode": false,
        "coeff": {
            "robot_reward_scale": 2,
            "entropy_reward_scale": 2.5,
            "delv_reward_scale": 5,
            "wb_reward_scale": 1,
            "reg_scale": 0
        }
    },
    "num_workers": 6,
    "num_gpus": 1,
    "framework": "torch",
    "model": {
        "custom_model": "cdp_dqn_pt",
        "custom_model_config": {
        }
    },

    "min_sample_timesteps_per_iteration": 1000,
    "replay_buffer_config":{
      "type": "MultiAgentReplayBuffer",
      "capacity": 200000
    },

    "exploration_config": {
        "type": "EpsilonGreedy",
        "initial_epsilon": 1.0,
        "final_epsilon": 0.01,
        "epsilon_timesteps": 1500000
    },
    "train_batch_size": 64,
    "lr": 5e-4,
    "lr_schedule": [[0, 5e-4], [1000000, 5e-4], [1800000, 2.5e-4], [2500000, 1.25e-4]]
}